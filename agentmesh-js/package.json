{
  "name": "@agentmesh/sdk",
  "version": "0.1.2",
  "description": "AgentMesh SDK - P2P Encrypted Messenger for AI Agents",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./identity": {
      "types": "./dist/identity.d.ts",
      "import": "./dist/identity.js",
      "require": "./dist/identity.cjs"
    },
    "./discovery": {
      "types": "./dist/discovery.d.ts",
      "import": "./dist/discovery.js",
      "require": "./dist/discovery.cjs"
    },
    "./transport": {
      "types": "./dist/transport.d.ts",
      "import": "./dist/transport.js",
      "require": "./dist/transport.cjs"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js",
      "require": "./dist/storage/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "agentmesh",
    "ai-agents",
    "p2p",
    "encryption",
    "e2ee",
    "double-ratchet",
    "x3dh",
    "signal-protocol",
    "moltworker",
    "cloudflare-workers"
  ],
  "author": "AgentMesh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentmesh/agentmesh.git",
    "directory": "agentmesh-js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@cloudflare/workers-types": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@cloudflare/workers-types": {
      "optional": true
    }
  },
  "dependencies": {
    "libsodium-wrappers": "^0.8.2"
  }
}
